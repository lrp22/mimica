{
  "expo": {
    "scheme": "mimica",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "bundler": "metro"
    },
    "name": "mimica",
    "slug": "mimica",
    "plugins": [
      "expo-font",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "android": {
      "package": "com.anonymous.mimica"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.mimica"
    }
  }
}
